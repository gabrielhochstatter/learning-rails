<% if user_signed_in? %>
<div>
  <%= link_to current_user.email, edit_user_registration_path %>
  <%= link_to 'Log Out', destroy_user_session_path, method: :delete %>
</div>
<% else %>
<div>
  <%= link_to 'Sign Up', new_user_registration_path %>
</div>
<div>
  <%= link_to 'Login', new_user_session_path %>
</div>
<% end %>
